{
  "name": "minimock-helper",
  "displayName": "Minimock helper",
  "publisher": "ikabirov",
  "description": "",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.103.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ikabirov/minimock-helper-vscode-extension.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "minimock-helper.generateMock",
        "title": "minimock: Generate mock file"
      },
      {
        "command": "minimock-helper.generateMockCmd",
        "title": "minimock: Add go:generate command"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "minimock-helper.generateMock",
          "when": "resourceExtname == .go",
          "group": "minimock"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}